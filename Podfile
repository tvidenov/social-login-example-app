# platform :ios, '9.0'

target 'FirebaseSocialLogin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

pod 'FBSDKCoreKit'
pod 'FBSDKShareKit'
pod 'FBSDKLoginKit'

pod 'Firebase/Auth'

pod 'GoogleSignIn'

end
